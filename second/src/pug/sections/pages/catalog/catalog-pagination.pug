nav(aria-label='Page navigation').m-t-40
  ul.pagination
    li
      a(href='#', aria-label='Previous')
        span(aria-hidden='true') &laquo;
    li.active
      a(href='#') 1
    li
      a(href='#') 2
    li
      a(href='#') 3
    li
      a(href='#') ...
    li
      a(href='#') 7
    li
      a(href='#', aria-label='Next')
        span(aria-hidden='true') &raquo;