        h2.section-subtitle.services-item__title Рерайт и копирайтинг
        .services-item__list.f-row.m-n-b-20.m-n-md-b-30
          +service-item('8.png', 'Автоматический (машинный) рерайт текста онлайн', '25 ₽/стр', '#')
          +service-item('9.jpg', 'Ручной (авторский) рерайт текста онлайн', '120 ₽/стр', '#')
